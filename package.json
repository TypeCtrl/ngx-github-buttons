{
  "name": "@ctrl/ngx-github-buttons",
  "version": "0.0.0-placeholder",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node build.ts",
    "ghpages": "ng build --prod --no-progress --baseHref='/ngx-github-buttons/'",
    "publish": "cd dist && npm publish --access public",
