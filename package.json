{
  "name": "@ctrl/ngx-github-buttons",
  "version": "0.0.0-placeholder",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node build.ts",
    "ghpages": "ng build --prod --no-progress --baseHref='/ngx-github-buttons/'",
    "publish": "cd dist && npm publish --access public",
    "test": "ng test --watch=false --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint ngx-github-buttons --type-check --format stylish",
    "lint:fix": "ng lint ngx-github-buttons --type-check --fix --format stylish",
    "bundlesize": "npx bundlesize",
    "travis-deploy-once": "travis-deploy-once",
    "commit": "git-cz",
    "semantic-release": "cd dist && semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.6",
    "@angular/common": "6.1.6",
    "@angular/compiler": "6.1.6",
    "@angular/core": "6.1.6",
    "@angular/forms": "6.1.6",
    "@angular/http": "6.1.6",
    "@angular/platform-browser": "6.1.6",
    "@angular/platform-browser-dynamic": "6.1.6",
    "@angular/router": "6.1.6",
    "bootstrap": "4.1.3",
    "core-js": "2.5.7",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.1.2",
    "@angular/compiler-cli": "6.1.6",
    "@angular/language-service": "6.1.6",
    "@angular-devkit/build-angular": "0.7.5",
    "@types/fs-extra": "5.0.4",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.5.6",
    "codelyzer": "4.4.2",
    "fs-extra": "7.0.0",
    "jasmine-core": "2.99.1",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-packagr": "4.0.1",
    "rimraf": "2.6.2",
    "semantic-release": "15.8.1",
    "travis-deploy-once": "5.0.1",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "2.9.2"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ctrl-ngx-github-buttons.umd.min.js",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/esm2015/ctrl-ngx-github-buttons.js",
      "maxSize": "6 kB"
    },
    {
      "path": "./dist/esm5/ctrl-ngx-github-buttons.js",
      "maxSize": "6 kB"
    }
  ]
}
