    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "5.1.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.12",
    "ts-node": "8.1.1",
